for a in range(1,151):
    for b in range(1,151):
        for c in range(1,151):
            for d in range(1,151):
                e = a ** 5 + b ** 5 + c ** 5 + d ** 5

                for f in range(1,151):
                    if e ** 5:
                        print(a, b, c, d)
                        print(f'Сумма = {e}')
